<br>
<h1>
  Warehouse
</h1>
<hr>
<% @orders.each do |order| %>
<div class="row">
  <div class="col-lg-12">
    <div class="panel panel-danger">
      <div class="panel-heading text-right">
        Due Date: <%= order.delivery_date %>
      </div>
      <div class="panel-body">
        New order received for a <%= order.name %>. Start the production
        for <%= order.batteries %> batteries.
      </div>
    </div>
  </div>
</div>
<% end %>
<hr>
